{
  "name": "distributed-electrons",
  "version": "1.0.0",
  "description": "Distributed Electrons - Multi-agent AI platform built on Cloudflare Workers",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,json,md}\"",
    "deploy-instance": "node scripts/deploy-instance.js",
    "deploy-all": "node scripts/deploy-all-instances.js",
    "wrangler:dev": "wrangler dev",
    "wrangler:deploy": "wrangler deploy",
    "db:migrate": "wrangler d1 execute DB --file=infrastructure/database/migrations/001-initial.sql",
    "db:seed": "wrangler d1 execute DB --file=infrastructure/database/seed.sql",
    "db:seed-models": "wrangler d1 execute DB --file=infrastructure/database/seed-models.sql",
    "db:migrate:oauth": "wrangler d1 execute DB --file=infrastructure/database/migrations/006-oauth.sql",
    "de-auth": "npx tsx scripts/de-auth.ts",
    "de-auth:status": "npx tsx scripts/de-auth.ts status",
    "de-auth:refresh": "npx tsx scripts/de-auth.ts refresh",
    "de-auth:deploy": "npx tsx scripts/de-auth.ts deploy",
    "build": "tsc && npm run lint && npm run test",
    "ci": "npm run typecheck && npm run lint && npm run test"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "multi-agent",
    "ai",
    "image-generation"
  ],
  "author": "Distributed Electrons Team",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^1.0.0",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.2",
    "vitest": "^1.0.0",
    "wrangler": "^4.53.0"
  }
}
