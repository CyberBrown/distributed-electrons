# Claude Runner Environment Configuration
# Copy to .env and fill in values

# Required: Secret for authenticating requests from sandbox-executor
# Generate with: openssl rand -hex 32
RUNNER_SECRET=

# Optional: Path to Claude credentials directory
# Defaults to ~/.claude
CLAUDE_CONFIG_PATH=~/.claude

# Optional: Config service URL for status updates
CONFIG_SERVICE_URL=https://config.distributedelectrons.com

# Optional: Internal API key for config service
INTERNAL_API_KEY=

# Optional: Cloudflare Tunnel token (if running cloudflared in Docker)
# CLOUDFLARE_TUNNEL_TOKEN=
